<h1>Edit Tag</h1>
<%= form_for(@tag) do |f| %>
  <ul>
  <% @tag.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to "<< Back to Tags", tags_path %><br>
<%= link_to "<< Back to Articles", articles_path %>
